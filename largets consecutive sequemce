def longest_consecutive_sequence(nums):
    s=set(nums)
    longest=0
    for n in nums:
        if n -1 not in s:
            length=0
            while n+length in s:
                length+=1
            longest=max(length,longest)
    return longest
nums=[100,4,200,1,3,2,4,5,3,4,2,3]
print(longest_consecutive_sequence(nums))        
    
