def pair_sum(arr,target):
    arr.sort()
    left=0
    right=len(arr)-1
    while left < right:
        current_sum=arr[left]+arr[right]
        if current_sum == target:
            return(arr[left],arr[right])
        elif current_sum<target:
           left+=1
        else:
            right-=1
            return None
arr=[1,2,3,4,6,7,8,9,2,7,6,3]
target=9
print(pair_sum(arr,target))